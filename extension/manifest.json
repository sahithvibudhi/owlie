{
    "name": "Owlie",
    "description": "A universal suggestion engine. find content based on what your friends like",
    "icons": {
        "16": "/images/owl.png",
        "32": "/images/owl.png",
        "48": "/images/owl.png",
        "128": "/images/owl.png"
    },
    "content_scripts":[
      {
        "js":["background-workers/injector.js"],
        "matches":["*://*.medium.com/*","*://www.youtube.com/*","*://*.netflix.com/*","*://*.spotify.com/*"]
      }
    ],
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage"],
    "action": {
      "default_popup": "pages/index.html",
      "default_icon": {
        "16": "/images/owl.png",
        "32": "/images/owl.png",
        "48": "/images/owl.png",
        "128": "/images/owl.png"
      }
    }
  }