{
    "name": "Owlie",
    "description": "A universal suggestion engine. find content based on what your friends like",
    "content_scripts":[
      {
        "js":["background-workers/injector.js"],
        "matches":["*://medium.com/","*://www.youtube.com/","*://www.netflix.com/","*://www.spotify.com/us/"]
      }
    ],
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage"],
    "action": {
      "default_popup": "pages/dashboard/index.html",
      "default_icon": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
      }
    }
  }